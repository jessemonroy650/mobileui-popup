<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "com.bsdmasterindex.mobileui-popup-test"
        version     = "1.0"
        versionCode = "10" >
        <gap:config-file platform="android" parent="/manifest/application" mode="merge">
            <application android:debuggable="false" />
        </gap:config-file>

        <!-- versionCode is optional and Android only -->
        <preference name="phonegap-version" value="3.5.0" />

        <name>MobileUI Popup</name>
        <description>MobileUI Popup</description>
        <author href="http://bsdmasterindex.com" email="jessemonroy650@yahoo.com">
        Jesse Monroy
        </author>
        <gap:platform name="android" />
        <!-- <gap:platform name="ios" /> -->

        <preference name="permissions" value="none"/>
        <preference name="orientation" value="landscape" />

        <!-- device required so we can detect if we are using iOS -->
        <gap:plugin name="org.apache.cordova.device" />

    </widget>
